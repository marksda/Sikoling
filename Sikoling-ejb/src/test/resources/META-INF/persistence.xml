<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
   version="3.0">
	<persistence-unit name="Sikoling-unit" transaction-type="JTA">
		<class>org.Sikoling.ejb.main.repository.sex.JenisKelaminData</class>
		<properties>
			<property name="jakarta.persistence.schema-generation.database.action" value="drop-and-create"/>
			<property name="jakarta.persistence.schema-generation.create-source" value="metadata"/>
			<property name="jakarta.persistence.schema-generation.sql-load-script-source" value="META-INF/data.sql"/>
		</properties>
	</persistence-unit>
</persistence>